    },
    "cell_4": {
      "cell_type": "code",
      "source": "# Set arrival processes\nsource.set_arrival(ocl1, Exp(1.0))  # Class1: arrival rate 1.0\nsource.set_arrival(ocl2, Exp(1.0))  # Class2: arrival rate 1.0",
      "metadata": {
        "ExecuteTime": {
          "end_time": "2025-07-30T22:22:48.095933Z",
