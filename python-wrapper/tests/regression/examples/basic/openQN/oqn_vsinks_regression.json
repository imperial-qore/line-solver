    "cell_3": {
      "cell_type": "code",
      "source": "# Define job classes\nocl1 = OpenClass(model, 'Class1')\nocl2 = OpenClass(model, 'Class2')",
      "metadata": {
        "ExecuteTime": {
          "end_time": "2025-07-30T22:22:48.045826Z",
