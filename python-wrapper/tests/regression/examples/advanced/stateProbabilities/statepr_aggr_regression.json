    "cell_2": {
      "cell_type": "code",
      "source": "# Block 2: classes\njobclass1 = ClosedClass(model, 'Class1', 2, node1, 0)  # 2 jobs\njobclass2 = ClosedClass(model, 'Class2', 0, node1, 0)  # 0 jobs (placeholder)",
      "metadata": {
        "ExecuteTime": {
          "end_time": "2025-07-15T22:46:43.061910Z",
